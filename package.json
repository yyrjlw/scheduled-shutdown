{
  "name": "scheduled-shutdown",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "tauri-dev": "tauri dev",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "tauri-build": "tauri build",
    "tauri-build:ARM64Linux": "tauri build --target aarch64-unknown-linux-gnu",
    "tauri-build:64-bitLinux": "tauri build --target x86_64-unknown-linux-gnu",
    "tauri-build:32-bitLinux": "tauri build --target i686-unknown-linux-gnu",
    "tauri-build:32-bitMinGW": "tauri build --target i686-pc-windows-gnu",
    "tauri-build:32-bitMSVC": "tauri build --target i686-pc-windows-msvc",
    "tauri-build:64-bitMinGW": "tauri build --target x86_64-pc-windows-gnu",
    "tauri-build:64-bitMSVC": "tauri build --target x86_64-pc-windows-msvc",
    "tauri-build:64-bitmacOS": "tauri build --target x86_64-apple-darwin",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.5.3",
    "animate.css": "^4.1.1",
    "dayjs": "^1.11.7",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@iconify-json/codicon": "^1.1.25",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tauri-apps/cli": "^1.5.10",
    "@tsconfig/node18": "^2.0.0",
    "@types/node": "^18.16.3",
    "@unocss/preset-icons": "^0.51.12",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.3.2",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "naive-ui": "^2.34.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "typescript": "~5.0.4",
    "unocss": "^0.51.8",
    "unplugin-vue-components": "^0.24.1",
    "vfonts": "^0.0.3",
    "vite": "^4.3.4",
    "vue-tsc": "^1.6.4"
  }
}
